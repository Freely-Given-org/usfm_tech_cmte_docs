          <ref name="Char"/>
          <ref name="Milestone"/>
          <ref name="Figure"/>
          <ref name="Verse"/>
          <ref name="Break"/>
          <optional>
            <text>
              <usfm:text strip="left"/>
            </text>
          </optional>
        </choice>
      </oneOrMore>
      <usfm:endtag id="a" many="?"/>
