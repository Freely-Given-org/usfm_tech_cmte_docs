  <define name="Char.char.style.enum">
    <choice>
      <value>va</value>
      <value>vp</value>
      <value>ca</value>
      <value>qac</value>
      <value>qs</value>
      <value>add</value>
      <value>addpn</value>
      <value>bk</value>
      <value>dc</value>
      <value>efm</value>
      <value>fm</value>
      <value>k</value>
      <value>nd</value>
      <value>ndx</value>
      <value>ord</value>
      <value>pn</value>
      <value>png</value>
      <value>pro</value>
      <value>qt</value>
      <value>rq</value>
      <value>sig</value>
      <value>sls</value>
      <value>tl</value>
      <value>wg</value>
      <value>wh</value>
      <value>wa</value>
      <value>wj</value>
      <value>xt</value>
      <value>jmp</value>
      <value>no</value>
      <value>it</value>
      <value>bd</value>
      <value>bdit</value>
      <value>em</value>
      <value>sc</value>
      <value>sup</value>
    </choice>
  </define>
  <define name="CharEmbed">
    <choice>
      <element>
        <name ns="">char</name>
        <attribute>
          <usfm:tag id="a" prefix="\+" grouping="9" stacked="true"/>
          <name ns="">style</name>
          <ref name="Char.char.style.enum"/>
        </attribute>
        <ref name="CharContent"/>
        <usfm:endtag id="a" many="?" usfm:order="3"/>
      </element>
      <ref name="CharWithAttrib.enum"/>
    </choice>
  </define>
  <define name="ListChar">
    <element>
      <name ns="">char</name>
      <attribute>
        <name ns="">style</name>
        <ref name="ListChar.char.style.enum"/>
      </attribute>
      <optional>
        <ref name="char.link"/>
      </optional>
      <optional>
        <ref name="char.closed"/>
      </optional>
      <text/>
      <zeroOrMore>
        <choice>
          <ref name="Reference"/>
          <ref name="Char"/>
          <ref name="Milestone"/>
          <ref name="Footnote"/>
          <ref name="Break"/>
          <text/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="ListChar.char.style.enum">
    <choice>
      <value>litl</value>
      <value>lik</value>
      <value>liv</value>
      <value>liv1</value>
      <value>liv2</value>
      <value>liv3</value>
      <value>liv4</value>
      <value>liv5</value>
    </choice>
  </define>
  <define name="CharWithAttrib.enum">
    <choice>
      <ref name="CharWithAttrib.char.style.w"/>
      <ref name="CharWithAttrib.char.style.rb"/>
    </choice>
  </define>
  <define name="CharWithAttrib.char.style.w">
    <element>
      <name ns="">char</name>
      <attribute>
        <usfm:tag id="a"/>
        <name ns="">style</name>
        <value>w</value>
      </attribute>
      <ref name="char.link"/>
      <optional>
        <ref name="char.closed"/>
      </optional>
      <optional>
        <attribute>
          <name ns="">lemma</name>
          <data type="string">
            <param name="minLength">1</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <name ns="">strong</name>
          <data type="string">
            <param name="pattern">([HG]\d{4,5}(:[a-z])?,?)+</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <name ns="">srcloc</name>
          <data type="string">
            <param name="pattern">[a-z]{3,5}\d?:\d+\.\d+\.\d+\.\d+</param>
          </data>
        </attribute>
      </optional>
      <text/>
      <zeroOrMore>
        <choice>
          <ref name="Reference"/>
          <ref name="Char"/>
          <ref name="Milestone"/>
          <ref name="Footnote"/>
          <ref name="Break"/>
          <text/>
        </choice>
      </zeroOrMore>
      <usfm:endtag id="a"/>
    </element>
  </define>
  <define name="CharWithAttrib.char.style.rb">
    <element>
      <name ns="">char</name>
      <attribute>
        <name ns="">style</name>
        <value>rb</value>
      </attribute>
      <optional>
        <ref name="char.link"/>
      </optional>
      <optional>
        <ref name="char.closed"/>
      </optional>
      <optional>
        <attribute>
          <name ns="">gloss</name>
          <data type="string">
            <param name="minLength">1</param>
          </data>
        </attribute>
      </optional>
      <text/>
      <zeroOrMore>
        <choice>
          <ref name="Reference"/>
          <ref name="Char"/>
          <ref name="Milestone"/>
          <ref name="Footnote"/>
          <ref name="Break"/>
          <text/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="char.link">
    <interleave>
      <optional>
        <attribute>
          <usfm:attrib>href</usfm:attrib>
          <name ns="">link-href</name>
          <data type="string">
            <param name="pattern">(.*///?(.*/?)+)|((prj:[A-Za-z\-0-9]{3,8} )?[A-Z1-4]{3} \d+:\d+(\-\d+)?)|(#[^\s]+)</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:attrib>title</usfm:attrib>
          <name ns="">link-title</name>
          <data type="string"/>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:attrib>id</usfm:attrib>
          <name ns="">link-id</name>
          <data type="string">
            <param name="pattern">[\p{L}\d_\-\.:]+</param>
          </data>
        </attribute>
      </optional>
      <usfm:repeat>
        <usfm:text noquotes="true">WS</usfm:text>
      </usfm:repeat>
    </interleave>
  </define>
  <define name="char.closed">
    <attribute>
      <name ns="">closed</name>
      <data type="boolean"/>
    </attribute>
  </define>
  <define name="Milestone">
    <choice>
      <element>
        <name ns="">ms</name>
        <attribute>
          <name ns="">style</name>
          <ref name="Milestone.ms.style.enum"/>
        </attribute>
        <optional>
          <attribute>
            <name ns="">sid</name>
            <data type="string">
              <param name="pattern">[\p{L}\d_\-\.:]+</param>
            </data>
          </attribute>
        </optional>
        <optional>
          <attribute>
            <name ns="">eid</name>
            <data type="string">
              <param name="pattern">[\p{L}\d_\-\.:]+</param>
            </data>
          </attribute>
        </optional>
        <empty/>
      </element>
      <ref name="MilestoneWithAttrib.enum"/>
    </choice>
  </define>
  <define name="Milestone.ms.style.enum">
    <choice>
      <data type="string">
        <param name="pattern">ts?(\-[se])?</param>
      </data>
      <data type="string">
        <param name="pattern">[Zz].+</param>
      </data>
    </choice>
  </define>
  <define name="MilestoneWithAttrib.enum">
    <ref name="MilestoneWithAttrib.ms.style.qt"/>
  </define>
  <define name="MilestoneWithAttrib.ms.style.qt">
    <element>
      <name ns="">ms</name>
      <attribute>
        <name ns="">style</name>
        <data type="string">
          <param name="pattern">qt[1-5]?(\-[se])?</param>
        </data>
      </attribute>
      <optional>
        <attribute>
          <name ns="">sid</name>
          <data type="string">
            <param name="pattern">[\p{L}\d_\-\.:]+</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <name ns="">eid</name>
          <data type="string">
            <param name="pattern">[\p{L}\d_\-\.:]+</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <name ns="">who</name>
          <data type="string">
            <param name="minLength">2</param>
          </data>
        </attribute>
      </optional>
      <empty/>
    </element>
  </define>
  <define name="Chapter">
    <choice>
      <ref name="ChapterStart"/>
      <ref name="ChapterEnd"/>
    </choice>
  </define>
  <define name="ChapterStart">
    <element>
      <name ns="">chapter</name>
      <interleave>
        <attribute>
          <usfm:tag>c</usfm:tag>
          <name ns="">style</name>
          <value>c</value>
        </attribute>
        <attribute>
          <usfm:text strip="both"/>
          <name ns="">number</name>
          <data type="integer"/>
        </attribute>
        <attribute>
          <name ns="">sid</name>
          <data type="string">
            <param name="minLength">1</param>
            <param name="pattern">[A-Z1-4]{3} ?[0-9]+</param>
          </data>
        </attribute>
        <optional>
          <attribute>
            <usfm:tag>ca</usfm:tag>
            <usfm:text strip="both"/>
            <usfm:endtag>ca</usfm:endtag>
            <name ns="">altnumber</name>
            <data type="string">
              <param name="minLength">1</param>
              <param name="pattern">[0-9]+\p{L}?(\x{200F}?[\-,][0-9]+\p{L}?)*</param>
            </data>
          </attribute>
        </optional>
        <optional>
          <attribute>
            <usfm:tag>cp</usfm:tag>
            <usfm:text strip="both"/>
            <usfm:endtag>cp</usfm:endtag>
            <name ns="">pubnumber</name>
            <data type="string">
              <param name="minLength">1</param>
            </data>
          </attribute>
        </optional>
        <empty/>
      </interleave>
    </element>
  </define>
  <define name="ChapterEnd">
    <element>
      <name ns="">chapter</name>
      <attribute>
        <name ns="">eid</name>
        <data type="string">
          <param name="minLength">1</param>
          <param name="pattern">[A-Z1-4]{3} ?[0-9]+</param>
        </data>
      </attribute>
      <empty/>
    </element>
  </define>
  <define name="BookChapterLabel">
    <element>
      <name ns="">para</name>
      <attribute>
        <usfm:tag>cl</usfm:tag>
        <name ns="">style</name>
        <value>cl</value>
      </attribute>
      <text>
        <usfm:text strip="both"/>
      </text>
    </element>
  </define>
  <define name="Verse">
    <choice>
      <ref name="VerseStart"/>
      <ref name="VerseEnd"/>
    </choice>
  </define>
  <define name="VerseStart">
    <element>
      <name ns="">verse</name>
      <attribute>
        <usfm:tag>v</usfm:tag>
        <name ns="">style</name>
        <value>v</value>
      </attribute>
      <attribute>
        <usfm:text strip="both?"/>
        <name ns="">number</name>
        <data type="string">
          <param name="minLength">1</param>
          <param name="pattern">[0-9]+[\p{L}\p{Mn}]*(\x{200F}?[\-,][0-9]+[\p{L}\p{Mn}]*)*</param>
        </data>
      </attribute>
      <optional>
        <attribute>
          <usfm:tag>va</usfm:tag>
          <usfm:text strip="both"/>
          <usfm:endtag>va</usfm:endtag>
          <name ns="">altnumber</name>
          <data type="string">
            <param name="minLength">1</param>
            <param name="pattern">[0-9]+[\p{L}\p{Mn}]*(\x{200F}?[\-,:][0-9]+[\p{L}\p{Mn}]*)*</param>
          </data>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:tag>vp</usfm:tag>
          <usfm:text strip="both"/>
          <usfm:endtag>vp</usfm:endtag>
          <name ns="">pubnumber</name>
          <data type="string">
            <param name="minLength">1</param>
          </data>
        </attribute>
      </optional>
      <attribute>
        <name ns="">sid</name>
        <data type="string">
          <param name="pattern">[A-Z1-4]{3} ?[a-z0-9\-:\p{L}\p{Mn}]*</param>
        </data>
      </attribute>
      <empty/>
    </element>
  </define>
  <define name="VerseEnd">
    <element>
      <name ns="">verse</name>
      <attribute>
        <name ns="">eid</name>
        <data type="string">
          <param name="pattern">[A-Z1-4]{3} ?[a-z0-9\-:\p{L}\p{Mn}]*</param>
        </data>
      </attribute>
      <empty/>
    </element>
  </define>
  <define name="Footnote">
    <element>
      <name ns="">note</name>
      <attribute>
        <usfm:tag id="a"/>
        <name ns="">style</name>
        <choice>
          <value>f</value>
          <value>fe</value>
          <value>ef</value>
        </choice>
      </attribute>
      <attribute>
        <usfm:text strip="right" re="\S+"/>
        <name ns="">caller</name>
        <text/>
      </attribute>
      <optional>
        <ref name="category"/>
      </optional>
      <optional>
        <ref name="FootnoteReference"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="FootnoteChar"/>
          <ref name="FootnoteReference"/>
        </choice>
      </oneOrMore>
      <usfm:endtag id="a"/>
    </element>
  </define>
  <define name="FootnoteChar">
    <element>
      <name ns="">char</name>
      <attribute>
        <usfm:tag id="a" grouping="10"/>
        <name ns="">style</name>
        <ref name="FootnoteChar.char.style.enum"/>
      </attribute>
      <optional>
        <ref name="char.link" usfm:ignore="true"/>
      </optional>
      <optional>
        <ref name="char.closed" usfm:ignore="true"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="CharEmbed"/>
          <ref name="FootnoteVerse"/>
          <text>
            <usfm:text/>
          </text>
        </choice>
      </oneOrMore>
      <usfm:endtag id="a" many="?" deprecated="true"/>
    </element>
  </define>
  <define name="FootnoteChar.char.style.enum">
    <choice>
      <value>ft</value>
      <value>fk</value>
      <value>fq</value>
      <value>fqa</value>
      <value>fl</value>
      <value>fw</value>
      <value>fp</value>
      <value>fv</value>
      <value>fdc</value>
      <value>xt</value>
      <value>it</value>
      <value>bd</value>
      <value>bdit</value>
      <value>em</value>
      <value>sc</value>
    </choice>
  </define>
  <define name="FootnoteVerse">
    <element>
      <name ns="">char</name>
      <attribute>
        <usfm:tag>fv</usfm:tag>
        <name ns="">style</name>
        <value>fv</value>
      </attribute>
      <text>
        <usfm:text/>
      </text>
      <usfm:endtag>fv</usfm:endtag>
    </element>
  </define>
  <define name="CrossReference">
    <element>
      <name ns="">note</name>
      <attribute>
        <name ns="">style</name>
        <choice>
          <value>x</value>
          <value>ex</value>
        </choice>
      </attribute>
      <attribute>
        <name ns="">caller</name>
        <text/>
      </attribute>
      <oneOrMore>
        <choice>
          <ref name="CrossReferenceChar"/>
          <text/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="CrossReferenceChar">
    <element>
      <name ns="">char</name>
      <attribute>
        <name ns="">style</name>
        <ref name="CrossReferenceChar.char.style.enum"/>
      </attribute>
      <optional>
        <ref name="char.link"/>
      </optional>
      <optional>
        <ref name="char.closed"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="Char"/>
          <ref name="CrossReferenceChar"/>
          <ref name="Reference"/>
          <text/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="CrossReferenceChar.char.style.enum">
    <choice>
      <value>xo</value>
      <value>xop</value>
      <value>xt</value>
      <value>xta</value>
      <value>xk</value>
      <value>xq</value>
      <value>xot</value>
      <value>xnt</value>
      <value>xdc</value>
    </choice>
  </define>
  <define name="Sidebar">
    <element>
      <name ns="">sidebar</name>
      <attribute>
        <name ns="">style</name>
        <value>esb</value>
      </attribute>
      <optional>
        <ref name="category"/>
      </optional>
      <oneOrMore>
        <choice>
          <ref name="Para"/>
          <ref name="List"/>
          <ref name="Table"/>
          <ref name="Footnote"/>
          <ref name="CrossReference"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="category">
    <attribute>
      <usfm:tag>cat</usfm:tag>
      <usfm:text strip="right*"/>
      <usfm:endtag>cat</usfm:endtag>
      <name ns="">category</name>
      <text/>
    </attribute>
  </define>
  <define name="Figure">
    <element>
      <usfm:tag>fig</usfm:tag>
      <name ns="">figure</name>
      <attribute>
        <usfm:attrib/>
        <name ns="">style</name>
        <text/>
      </attribute>
      <optional>
        <attribute>
          <usfm:attrib/>
          <name ns="">alt</name>
          <text/>
        </attribute>
      </optional>
      <attribute>
        <usfm:attrib/>
        <name ns="">file</name>
        <text/>
      </attribute>
      <optional>
        <attribute>
          <usfm:attrib/>
          <name ns="">size</name>
          <text/>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:attrib/>
          <name ns="">loc</name>
          <text/>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:attrib/>
          <name ns="">copy</name>
          <text/>
        </attribute>
      </optional>
      <optional>
        <attribute>
          <usfm:attrib/>
          <name ns="">ref</name>
          <text/>
        </attribute>
      </optional>
      <optional>
        <text>
          <usfm:attrib key="caption"/>
        </text>
      </optional>
    </element>
  </define>
  <define name="FootnoteReference">
    <element>
      <name ns="">ref</name>
      <attribute usfm:order="2">
        <usfm:text strip="left?">|</usfm:text>
        <usfm:text/>
        <usfm:endtag many="?" deprecated="true">fr</usfm:endtag>
        <name ns="">loc</name>
        <data type="string">
          <param name="pattern">[A-Z1-4]{3}(-[A-Z1-4]{3})? ?[a-z0-9\-:]*</param>
        </data>
      </attribute>
      <usfm:tag strip="none">fr</usfm:tag>
      <optional>
        <text>
          <usfm:text strip="left"/>
        </text>
      </optional>
    </element>
  </define>
  <define name="Break">
    <element>
      <name ns="">optbreak</name>
      <empty>
        <usfm:tag lookahead="\">b</usfm:tag>
      </empty>
    </element>
  </define>
</grammar>
